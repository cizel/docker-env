[program:nginx]
directory=/apps/srv/nginx/sbin/
command=/apps/srv/nginx/sbin/nginx
user=root
priority=999
numprocs=1
autostart=true
autorestart=true
startsecs=3
startretries=2
stopsignal=KILL
stopwaitsecs=10
stdout_logfile=/apps/logs/supervisor/keycenter.log
stderr_logfile=/apps/logs/supervisor/keycenter.err
logfile_maxbytes=1MB
logfile_backups=10

